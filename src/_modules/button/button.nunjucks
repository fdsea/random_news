{% macro content(spec) %}

{% set spec = spec or {} %}
{% set _id = spec.id or '' %}
{% set _class = spec.class or '' %}
{% set _text = spec.text or 'Jast New Btn' %}
{% set _type = spec.type or '' %}

<button
  id="{{_id}}"
  class="{{_class}}"
  type="{{_type}}"
>
{{_text}}
</button>
{% endmacro %}